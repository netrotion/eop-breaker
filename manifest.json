  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "*://eop.edu.vn/*",
        "*://eop.edu.vn/*",
        "*://eop.edu.vn/*"
      ],
      "resources": [
        "assets/message-util.js",
